{
	"build": {
		"env": {
			"NODE_ENV": "production"
		}
	},
	"routes": [
		{
			"src": "/api/stories",
			"methods": ["POST", "PUT", "DELETE"],
			"status": 404
		},
		{
			"src": "/api/stories/(.*)",
			"methods": ["POST", "PUT", "DELETE"],
			"status": 404
		}
	],
	"outputDirectory": "dist",
	"ignoreCommand": "npx vite build --mode production"
}
