# Example environment configuration
# Copy this to .env.local and fill in your values

NODE_ENV=development

# Database configuration (read-only user recommended)
DATABASE_URL="postgresql://your_read_only_user:password@your_host/database"
DATABASE_URL_UNPOOLED="postgresql://your_read_only_user:password@your_host/database"
NEON_PROJECT_ID="your_project_id"

# For admin functionality (not included in this repo)
ADMIN_SECRET_KEY="your_admin_secret_key"
VITE_ADMIN_ENABLED=false
